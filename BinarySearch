#include<stdio.h>
int main()
{
    int i,num,a[30],low=0,high=9,mid,key;

    printf("Enter the array elements in ascending order \n");
    for(i=0;i<10;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Enter the element to search \n");
    scanf("%d",&key);
    mid=(low+high)/2;
    while(low<=high && a[mid]!=key)
    {
        if(key<a[mid])
        {
            high=mid-1;
        }
        else
            low=mid+1;
        mid=(low+high)/2;

    }
    if(a[mid]==key)
        printf("found = %d",key);
    else
        printf("not found");

    return 0;
}
