#include<stdio.h>
int main()
{
        int a,b,num;
        printf("Enter two digits \n");
        scanf("%d%d",&a,&b);
        printf(" 1. sum\n 2. subtraction\n 3. multiplicaton\n 4. division\n 5. exit\n");
        scanf("%d",&num);
        switch(num)
        {
            case 1: printf("sum of two numbers = %d",a+b);
                    break;
            case 2: printf("subtraction of two numbers = %d",a-b);
                    break;
            case 3: printf("multiplication of two numbers = %d",a*b);
                    break;
            case 4: printf("division of two numbers = %d",a/b);
                    break;
            default:exit(0);
        }
    return 0;
}

